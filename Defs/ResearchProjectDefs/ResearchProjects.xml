    <ResearchProjectDef>
        <defName>IntegratedLogisticsContainer</defName>
        <label>integrated logistics container</label>
        <description>Research a container capable of storing all items, including corpses and waste.</description>
        <baseCost>4000</baseCost>
        <tab>Logistics</tab>
        <researchViewX>5.00</researchViewX>
        <researchViewY>2.80</researchViewY>
        <techLevel>Industrial</techLevel>
        <requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
        <requiredResearchFacilities>
            <li>MultiAnalyzer</li>
        </requiredResearchFacilities>
        <prerequisites>
            <li>LogisticsNetwork</li>
            <li>MassiveLogisticsSystem</li>
        </prerequisites>
    </ResearchProjectDef>
</Defs>