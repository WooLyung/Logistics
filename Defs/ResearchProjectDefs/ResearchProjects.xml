<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <ResearchProjectDef>
        <defName>BasicLogistics</defName>
        <label>basic logistics</label>
        <description>Research basic devices for the logistics system. The logistics system is
            composed of containers and interfaces connected underground, centered around the
            logistics system controller</description>
        <baseCost>2000</baseCost>
        <tab>Logistics</tab>
        <researchViewX>0.00</researchViewX>
        <researchViewY>1.40</researchViewY>
        <techLevel>Industrial</techLevel>
        <requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
        <prerequisites>
            <li>Electricity</li>
        </prerequisites>
    </ResearchProjectDef>

    <ResearchProjectDef>
        <defName>AdvancedLogistics</defName>
        <label>advanced logistics</label>
        <description>Build a more efficient and improved logistics system.</description>
        <baseCost>3000</baseCost>
        <tab>Logistics</tab>
        <researchViewX>2.00</researchViewX>
        <researchViewY>1.40</researchViewY>
        <techLevel>Industrial</techLevel>
        <requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
        <requiredResearchFacilities>
            <li>MultiAnalyzer</li>
        </requiredResearchFacilities>
        <prerequisites>
            <li>BasicLogistics</li>
        </prerequisites>
    </ResearchProjectDef>

    <ResearchProjectDef>
        <defName>LogisticsNetwork</defName>
        <label>logistics network</label>
        <description>Connect multiple logistics systems to form a large-scale network.</description>
        <baseCost>5000</baseCost>
        <tab>Logistics</tab>
        <researchViewX>4.00</researchViewX>
        <researchViewY>1.40</researchViewY>
        <techLevel>Spacer</techLevel>
        <requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
        <requiredResearchFacilities>
            <li>MultiAnalyzer</li>
        </requiredResearchFacilities>
        <prerequisites>
            <li>AdvancedLogistics</li>
        </prerequisites>
    </ResearchProjectDef>

    <ResearchProjectDef>
        <defName>LargeLogisticsSystem</defName>
        <label>large logistics system</label>
        <description>Expand containers to build a more densely packed logistics system.</description>
        <baseCost>1000</baseCost>
        <tab>Logistics</tab>
        <researchViewX>1.00</researchViewX>
        <researchViewY>2.80</researchViewY>
        <techLevel>Industrial</techLevel>
        <requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
        <prerequisites>
            <li>BasicLogistics</li>
        </prerequisites>
    </ResearchProjectDef>

    <ResearchProjectDef>
        <defName>MassiveLogisticsSystem</defName>
        <label>massive logistics system</label>
        <description>Further expand containers to construct a highly compact logistics system.</description>
        <baseCost>2000</baseCost>
        <tab>Logistics</tab>
        <researchViewX>3.00</researchViewX>
        <researchViewY>2.80</researchViewY>
        <techLevel>Industrial</techLevel>
        <requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
        <requiredResearchFacilities>
            <li>MultiAnalyzer</li>
        </requiredResearchFacilities>
        <prerequisites>
            <li>LargeLogisticsSystem</li>
            <li>AdvancedLogistics</li>
        </prerequisites>
    </ResearchProjectDef>

    <ResearchProjectDef>
        <defName>LogisticsAutoExporter</defName>
        <label>logistics auto exporter</label>
        <description>Develop an auto-exporter that automatically retrieves items from the logistics system.</description>
        <baseCost>1200</baseCost>
        <tab>Logistics</tab>
        <researchViewX>1.00</researchViewX>
        <researchViewY>0.70</researchViewY>
        <techLevel>Industrial</techLevel>
        <requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
        <prerequisites>
            <li>BasicLogistics</li>
        </prerequisites>
    </ResearchProjectDef>

    <ResearchProjectDef>
        <defName>LogisticsInputPort</defName>
        <label>logistics input port</label>
        <description>Research a device that allows items to be fed into an adjacent logistics system.</description>
        <baseCost>1200</baseCost>
        <tab>Logistics</tab>
        <researchViewX>1.00</researchViewX>
        <researchViewY>0.00</researchViewY>
        <techLevel>Industrial</techLevel>
        <requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
        <prerequisites>
            <li>BasicLogistics</li>
        </prerequisites>
    </ResearchProjectDef>

    <ResearchProjectDef>
        <defName>ConveyorInterface</defName>
        <label>conveyor interface</label>
        <description>Expands the logistics system through conveyor-based connections.</description>
        <baseCost>1000</baseCost>
        <tab>Logistics</tab>
        <researchViewX>3.00</researchViewX>
        <researchViewY>2.10</researchViewY>
        <techLevel>Industrial</techLevel>
        <requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
        <prerequisites>
            <li>AdvancedLogistics</li>
        </prerequisites>
        <requiredResearchFacilities>
            <li>MultiAnalyzer</li>
        </requiredResearchFacilities>
    </ResearchProjectDef>

    <ResearchProjectDef>
        <defName>IntegratedInputOutput</defName>
        <label>integrated input/output</label>
        <description>Researches an interface that combines both input and output into a single unit.</description>
        <baseCost>1500</baseCost>
        <tab>Logistics</tab>
        <researchViewX>1.00</researchViewX>
        <researchViewY>2.10</researchViewY>
        <techLevel>Industrial</techLevel>
        <requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
        <prerequisites>
            <li>BasicLogistics</li>
        </prerequisites>
    </ResearchProjectDef>

    <ResearchProjectDef>
        <defName>LogisticsSmartExporter</defName>
        <label>logistics smart exporter</label>
        <description>Determines the export amount based on the state of the logistics system.</description>
        <baseCost>2200</baseCost>
        <tab>Logistics</tab>
        <researchViewX>3.00</researchViewX>
        <researchViewY>0.70</researchViewY>
        <techLevel>Industrial</techLevel>
        <requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
        <requiredResearchFacilities>
            <li>MultiAnalyzer</li>
        </requiredResearchFacilities>
        <prerequisites>
            <li>LogisticsAutoExporter</li>
            <li>AdvancedLogistics</li>
        </prerequisites>
    </ResearchProjectDef>

    <ResearchProjectDef>
        <defName>RemoteInterface</defName>
        <label>remote interface</label>
        <description>An interface that operates within a logistics network completely separate from the primary logistics system.</description>
        <baseCost>4000</baseCost>
        <tab>Logistics</tab>
        <researchViewX>5.00</researchViewX>
        <researchViewY>2.10</researchViewY>
        <techLevel>Spacer</techLevel>
        <requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
        <requiredResearchFacilities>
            <li>MultiAnalyzer</li>
        </requiredResearchFacilities>
        <prerequisites>
            <li>ConveyorInterface</li>
            <li>LogisticsNetwork</li>
        </prerequisites>
    </ResearchProjectDef>

    <ResearchProjectDef>
        <defName>LogisticsRemoteExporter</defName>
        <label>logistics remote exporter</label>
        <description>An export unit that operates independently from the main logistics system.</description>
        <baseCost>4000</baseCost>
        <tab>Logistics</tab>
        <researchViewX>5.00</researchViewX>
        <researchViewY>0.70</researchViewY>
        <techLevel>Spacer</techLevel>
        <requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
        <requiredResearchFacilities>
            <li>MultiAnalyzer</li>
        </requiredResearchFacilities>
        <prerequisites>
            <li>LogisticsSmartExporter</li>
            <li>LogisticsNetwork</li>
        </prerequisites>
    </ResearchProjectDef>

    <ResearchProjectDef>
        <defName>AutoTransferNode</defName>
        <label>auto transfer node</label>
        <description>Automatically transfers nearby items into the connected logistics system.</description>
        <baseCost>4000</baseCost>
        <tab>Logistics</tab>
        <researchViewX>5.00</researchViewX>
        <researchViewY>0.00</researchViewY>
        <techLevel>Spacer</techLevel>
        <requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
        <requiredResearchFacilities>
            <li>MultiAnalyzer</li>
        </requiredResearchFacilities>
        <prerequisites>
            <li>LogisticsNetwork</li>
        </prerequisites>
    </ResearchProjectDef>

    <ResearchProjectDef>
        <defName>LogisticsIncinerator</defName>
        <label>logistics incinerator</label>
        <description>Automatically disposes of designated items from the logistics system by incineration.</description>
        <baseCost>2500</baseCost>
        <tab>Logistics</tab>
        <researchViewX>3.00</researchViewX>
        <researchViewY>0.00</researchViewY>
        <techLevel>Industrial</techLevel>
        <requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
        <requiredResearchFacilities>
            <li>MultiAnalyzer</li>
        </requiredResearchFacilities>
        <prerequisites>
