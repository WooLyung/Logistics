<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <ThingDef ParentName="LogisticsInnerInterfaceBase">
        <defName>LogisticsInputInterface</defName>
        <label>logistics input interface</label>
        <description>An interface for inserting items into logistics containers. It can be used in logistics warehouses that include a logistics system controller.</description>
        <graphicData>
            <texPath>Things/Building/LogisticsInputInterface</texPath>
        </graphicData>
        <uiOrder>200</uiOrder>
        <comps>
            <li Class="Logistics.CompProperties_InputInterface"/>
            <li Class="CompProperties_Power">
                <compClass>CompPowerTrader</compClass>
                <basePowerConsumption>100</basePowerConsumption>
                <shortCircuitInRain>true</shortCircuitInRain>
            </li>
        </comps>
        <researchPrerequisites>
            <li>BasicLogistics</li>
        </researchPrerequisites>
        <statBases>
            <WorkToBuild>1000</WorkToBuild>
        </statBases>
        <costList>
            <Steel>30</Steel>
            <ComponentIndustrial>1</ComponentIndustrial>
            <ConveyorBelt>1</ConveyorBelt>
        </costList>
        <constructionSkillPrerequisite>6</constructionSkillPrerequisite>
    </ThingDef>

